<%- include('../partials/header') %>
<%- include('../partials/navbar') %>
<main>
    <h1>Home Pages</h1>
    <h2>Messages</h2>
    <div class="card">
        <% messages.forEach( (message) => { %>
        <div class="card">
            <p><%= message.text %></p>
            <p>From: <%= message.user %></p>
            <p>Posted on: <%= message.added %></p>
            <p><a href="/messages/<%= message.id%>">Open</a></p>
        </div>
        <% }) %>
    </div>
</main>

<%- include('../partials/footer') %>